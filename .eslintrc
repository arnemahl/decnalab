{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "ecmaFeatures": {
        "classes": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    },
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "DEBUG": false
    },
    "rules": {
        "arrow-spacing": 1,
        "block-spacing": 1,
        "brace-style": 1,
        "camelcase": 0,
        "curly": 1,
        "comma-dangle": 0,
        "comma-spacing": 1,
        "dot-notation": 0,
        "dot-location": [1, "property"],
        "eol-last": 1,
        "eqeqeq": 1,
        "indent": [ 2, 4, { "SwitchCase": 1 } ],
        "key-spacing": 0,
        "max-len": [ 1, 180, 4, { "ignoreComments": true } ],
        "newline-after-var": 0,
        "no-cond-assign": 1,
        "no-constant-condition": 1,
        "no-const-assign": 1,
        "no-return-assign": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty": 1,
        "no-extra-bind": 1,
        "no-extra-semi": 1,
        "no-loop-func": 0,
        "no-mixed-requires": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": [ 1, { "max": 3 } ],
        "no-multi-spaces": [
            1,
            {
                "exceptions": {
                    "VariableDeclarator": true
                }
            }
        ],
        "no-redeclare": 1,
        "no-sequences": 1,
        "no-trailing-spaces": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 2,
        "no-unreachable": 1,
        "no-useless-call": 1,
        "no-use-before-define": 1,
        "no-var": 1,
        "no-unused-vars": 1,
        "no-with": 2,
        "object-shorthand": 1,
        "operator-assignment": 2,
        "operator-linebreak": 0,
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-spread": 1,
        "quotes": 0,
        "semi": 0,
        "keyword-spacing": 1,
        "space-before-blocks": 1,
        "space-infix-ops": 0,
        "strict": 2
    }
}
